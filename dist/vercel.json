{
  "rewrites": [
    { "source": "/:bundleid", "destination": "/index.html" },
    { "source": "/:bundleid/", "destination": "/index.html" },
    { "source": "/:bundleid/:cc", "destination": "/index.html" },
    { "source": "/:bundleid/:cc/", "destination": "/index.html" },
    { "source": "/:bundleid/:cc/:lang", "destination": "/index.html" },
    { "source": "/:bundleid/:cc/:lang/", "destination": "/index.html" }
  ],
  "headers": [
    { "source": "/(.*)", "headers": [{ "key": "Cache-Control", "value": "public, max-age=14400, s-maxage=14400, stale-while-revalidate=14400" }] }
  ]
}